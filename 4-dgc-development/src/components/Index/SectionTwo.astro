---
import Container from '../Container.astro';

const data = [
  {
    title: 'High Quality',
    description: 'Unmatched Excellence in Truss Solutions.',
    logo: 'data:image/svg+xml;base64,',
  },
  {
    title: 'Effienciency',
    description: 'In everything we do we are commitment to excellence.',
    logo: 'data:image/svg+xml;base64,',
  },
  {
    title: 'Skilled personnel',
    description: 'We have the best team comprised of top professionals.',
    logo: 'data:image/svg+xml;base64,',
  },
];
---

<section id="Section" class="relative overflow-hidden">
  <Container>
    <article class="flex flex-col items-center justify-center relative z-10 gap-10 py-32">
      <h4 class="text-6xl text-gray-900 font-bold font-display">Reliable Business</h4>
      <div class="grid md:grid-cols-3 gap-4 lg:gap-[4vw] w-3/4">
        {
          data.map((itm) => (
            <div class="bg-[#BB954C] hover:scale-105 transition  rounded-3xl p-6 w-full h-full grid place-items-center cursor-pointer">
              <img class="mx-auto max-h-24 mb-6" src={itm.logo} alt="" />
              <footer class="text-center max-w-[175px]">
                <h6 class="text-xl text-white font-semibold font-display">{itm.title}</h6>
              </footer>
            </div>
          ))
        }
      </div>
    </article>
  </Container>
  <style></style>
</section>
