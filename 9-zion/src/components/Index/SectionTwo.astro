---
import Container from '../Container.astro';

const data = [
  {
    title: 'High Quality',
    description: 'Unmatched Excellence in Finance Solutions.',
    logo: 'data:image/svg+xml;base64,',
  },
  {
    title: 'Effienciency',
    description: 'Unmatched Excellence in Finance Solutions.',
    logo: 'data:image/svg+xml;base64,',
  },
  {
    title: 'Skilled personnel',
    description: 'Unmatched Excellence in Finance Solutions.',
    logo: 'data:image/svg+xml;base64,',
  },
];
---

<section id="Section" class="relative overflow-hidden bg-[#01499D]">
  <!-- <img
    class="absolute h-full w-full object-cover brightness-50"
    src="https://www.soracom.io/wp-content/uploads/2022/03/AdobeStock_462845394-1-scaled.jpeg"
    alt=""
  /> -->
  <Container>
    <article class="flex flex-col items-center justify-center relative z-10 gap-10 py-32">
      <div class="text-center max-w-lg text-white">
        <h4 class="font-bold text-6xl font-sans">Finance</h4>
      </div>
      <div class="grid md:grid-cols-3 gap-4 lg:gap-[4vw]">
        {
          data.map((itm) => (
            <div>
              <div class="bg-white aspect-square rounded-xl p-8 max-w-[144px] flex justify-center items-center mx-auto">
                <img class="my-auto" src={itm.logo} alt="" />
              </div>
              <footer class="text-center max-w-[155px] mt-2">
                <h6 class="text-xl text-white font-semibold">{itm.title}</h6>
                <h6 class=" text-center text-xs text-white/60">{itm.description}</h6>
              </footer>
            </div>
          ))
        }
      </div>
    </article>
  </Container>
  <style></style>
</section>
